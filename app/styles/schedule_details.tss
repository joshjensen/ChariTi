"#Wrapper": {
	backgroundColor: "#FFF"
},
"#content": {
	layout: "vertical",
	top: "47dp",
	showVerticalScrollIndicator: true
},
"#header": {
	backgroundGradient: {
		type: "linear",
		colors: [
			"#F8F8F8",
			"#ECECEC"
		],
		startPoint: {
			x: 0,
			y: 0
		},
		endPoint: {
			x: 0,
			y: "100%"
		},
		backFillStart: true
	},
	height: Ti.UI.SIZE,
	width: Ti.UI.FILL,
	layout: "vertical"
},
"#date": {
	top: "10dp",
	left: "10dp",
	right: "10dp",
	height: "14dp",
	font: {
		fontSize: "12dp",
		fontWeight: "bold"
	},
	color: "#777"
},
"#heading": {
	top: "3dp",
	left: "10dp",
	right: "10dp",
	height: Ti.UI.SIZE,
	font: {
		fontSize: "16dp",
		fontWeight: "bold"
	},
	color: "#333"
},
"#speaker": {
	top: "3dp",
	left: "10dp",
	right: "10dp",
	bottom: "2dp",
	height: Ti.UI.SIZE,
	font: {
		fontSize: "14dp",
		fontWeight: "normal"
	},
	color: "#333"
},
"#location": {
	top: "4dp",
	left: "10dp",
	right: "10dp",
	bottom: "10dp",
	height: "14dp",
	font: {
		fontSize: "12dp",
		fontWeight: "normal"
	},
	color: "#333"
},
"#image": {
	top: "10dp",
	width: Ti.UI.SIZE,
	height: Ti.UI.SIZE
},
"#text": {
	top: "10dp",
	left: "15dp",
	right: "15dp",
	bottom: "15dp",
	height: Ti.UI.SIZE,
	font: {
		fontSize: "14dp",
		fontWeight: "normal"
	},
	color: "#777",
	backgroundColor: "transparent",
	editable: false,
	autoLink: Ti.UI.AUTOLINK_URLS
}